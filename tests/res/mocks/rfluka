#!/bin/bash

FILE_NAME_0="cherenkov001_fort.55"
FILE_NAME_CONTENT_0=""
sleep 1
printf '%s' "$FILE_NAME_CONTENT_0" | base64 -d > $FILE_NAME_0
echo "Generated $FILE_NAME_0"

FILE_NAME_1="cherenkov002_fort.55"
FILE_NAME_CONTENT_1=""
sleep 1
printf '%s' "$FILE_NAME_CONTENT_1" | base64 -d > $FILE_NAME_1
echo "Generated $FILE_NAME_1"

FILE_NAME_2="cherenkov003_fort.55"
FILE_NAME_CONTENT_2=""
sleep 1
printf '%s' "$FILE_NAME_CONTENT_2" | base64 -d > $FILE_NAME_2
echo "Generated $FILE_NAME_2"

FILE_NAME_3="cherenkov004_fort.55"
